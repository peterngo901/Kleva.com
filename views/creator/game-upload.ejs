        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox9\" value=\"Texts in context\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox9\">Texts in context</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox10\" value=\"Interacting with others\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox10\">Interacting with others</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox11\" value=\"Interpreting, analysing, evaluating\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox11\">Interpreting, analysing, evaluating</label>\
      </div>'
    } else if (fired_button === 'digital technologies') {
      return document.getElementById('subcat').innerHTML = '<h1>Digital Technologies</h1>\
      <input type=\"hidden\" name=\"category\" id=\"category\" value=\"digitalTechnology\">\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox1\" value=\"Design and Technologies Knowledge and Understanding\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox1\">Design and Technologies Knowledge and Understanding</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox2\" value=\"Design and Technologies Processes and Production Skills\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox2\">Design and Technologies Processes and Production Skills</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox3\" value=\"Digital Technologies Knowledge and Understanding\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox3\">Digital Technologies Knowledge and Understanding</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox4\" value=\"Digital Technologies Processes and Production Skills\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox4\">Digital Technologies Processes and Production Skills</label>\
      </div>'
    } else if (fired_button === 'arts') {
      return document.getElementById('subcat').innerHTML = '<h1>Arts</h1>\
      <input type=\"hidden\" name=\"category\" id=\"category\" value=\"arts\">\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox1\" value=\"Dance\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox1\">Dance</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox2\" value=\"Drama\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox2\">Drama</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox3\" value=\"Media Artworks\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox3\">Media Artworks</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox4\" value=\"Music\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox4\">Music</label>\
      </div>'
    } else if (fired_button === 'hass') {
      return document.getElementById('subcat').innerHTML = '<h1>Humanities</h1>\
      <input type=\"hidden\" name=\"category\" id=\"category\" value=\"hass\">\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox1\" value=\"History\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox1\">History</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox2\" value=\"Geography\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox2\">Geography</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox3\" value=\"Civics and Citizenship\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox3\">Civics and Citizenship</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox4\" value=\"Economics and Business\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox4\">Economics and Business</label>\
      </div>'
    }
  });
</script>

</html>