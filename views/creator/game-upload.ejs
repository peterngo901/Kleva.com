    } else if (fired_button === 'science') {
      return document.getElementById('subcat').innerHTML = '<h1>Science</h1>\
      <input type=\"hidden\" name=\"category\" id=\"category\" value=\"science\">\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox1\" value=\"Biological sciences\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox1\">Biological sciences</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox2\" value=\"Earth and space sciences\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox2\">Earth and space sciences</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox3\" value=\"Chemical sciences\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox3\">Chemical sciences</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox4\" value=\"Physical sciences\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox4\">Physical sciences</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox5\" value=\"Nature and development  of science\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox5\">Nature and development  of science</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox6\" value=\"Use and influence of science\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox6\">Use and influence of science</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox7\" value=\"Questioning and predicting\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox7\">Questioning and predicting</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox8\" value=\"Planning and conducting\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox8\">Planning and conducting</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox9\" value=\"Processing and analysing data and information\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox9\">Processing and analysing data and information</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox10\" value=\"Evaluating\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox10\">Evaluating</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox11\" value=\"Communicating\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox11\">Communicating</label>\
      </div>'
    } else if (fired_button === 'english') {
      return document.getElementById('subcat').innerHTML = '<h1>English</h1>\
      <input type=\"hidden\" name=\"category\" id=\"category\" value=\"english\">\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox1\" value=\"Text structure and organisation\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox1\">Text structure and organisation</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox2\" value=\"Language variation and change\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox2\">Language variation and change</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox3\" value=\"Language for interaction\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox3\">Language for interaction</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox4\" value=\"Expressing and developing ideas\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox4\">Expressing and developing ideas</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox5\" value=\"Literature and context\">\
          <label class=\"form-check-label\" for=\"inlineCheckbox5\">Literature and context</label>\
      </div>\
      <div class=\"checkbox-inline\">\
        <input class=\"form-check-input\" type=\"checkbox\" name=\"subCategories\" id=\"inlineCheckbox6\" value=\"Responding to literature\">\
