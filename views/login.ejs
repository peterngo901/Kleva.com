<%- include('reusables/head.ejs') %>
        <link rel="stylesheet" href="css/creator-dashboard.css">
        <title><%= pageTitle %></title>
    </head>

    <body>
            <%- include('reusables/navigation.ejs') %>

            <div class="content">

                <!-- Main Content Area -->
                <main class="upload">
                    <!-- Game Upload Form -->
                    <div class="upload-form">
                        <form action="/postUploadGames" class="upload-form__text" method="POST">
                            <!-- Username -->
                            <label class="upload-form__title" for="username">Username</label>
                            <input class="upload-form__title-field" type="text" id="username" name="username" placeholder="Username"><br>
                            
                            <!-- Password -->
                            <label class="upload-form__title" for="title">Password</label>
                            <input class="upload-form__title-field" type="text" id="password" name="password" placeholder="Password"><br>
                            
                            <!-- Submission Button -->
                            <input class="upload-form__button" type="submit" value="Login">
                        </form>
                    </div>
                </main>   
            </div>
        </div>
    </body>

</html>
